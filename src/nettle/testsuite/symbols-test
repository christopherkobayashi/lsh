#! /bin/sh

# Check that all exported symbols use the nettle prefix.

if [ -z "$srcdir" ] ; then
  srcdir=`pwd`
fi

nm ../libnettle.a | grep ' [DRT] ' | egrep -v ' \.?(_?nettle_|memxor)' \
    | sort -k3 > test1.out

if [ -s test1.out ] ; then
    echo Exported symbols lacking the nettle prefix:
    cat test1.out
    exit 1
else
    exit 0
fi
